{
	"name": "mpetrovich/dash",
	"description": "A functional utility library for PHP, like Underscore/Lodash",
	"keywords": ["php", "functional programming", "functional", "underscore", "lodash", "underbar", "bottomline", "ramda"],
	"homepage": "https://github.com/mpetrovich/Dash",
	"license": "MIT",
	"authors": [
		{
			"name": "Mike Petrovich",
			"email": "michael.c.petrovich@gmail.com"
		}
	],
	"require": {
		"php": ">=5.4.0"
	},
	"autoload": {
		"psr-4": {
			"Dash\\": ["src/", "tests/"]
		},
		"files": [
			"src/all.php",
			"src/any.php",
			"src/assertType.php",
			"src/at.php",
			"src/average.php",
			"src/compare.php",
			"src/contains.php",
			"src/deltas.php",
			"src/difference.php",
			"src/each.php",
			"src/equal.php",
			"src/every.php",
			"src/filter.php",
			"src/find.php",
			"src/findKey.php",
			"src/findLast.php",
			"src/findValue.php",
			"src/first.php",
			"src/get.php",
			"src/groupBy.php",
			"src/identical.php",
			"src/identity.php",
			"src/indexBy.php",
			"src/intersection.php",
			"src/isEmpty.php",
			"src/isEven.php",
			"src/isOdd.php",
			"src/keyBy.php",
			"src/keys.php",
			"src/last.php",
			"src/map.php",
			"src/mapValues.php",
			"src/matches.php",
			"src/matchesProperty.php",
			"src/max.php",
			"src/median.php",
			"src/min.php",
			"src/negate.php",
			"src/partial.php",
			"src/partialRight.php",
			"src/pick.php",
			"src/pluck.php",
			"src/property.php",
			"src/reduce.php",
			"src/reject.php",
			"src/reverse.php",
			"src/set.php",
			"src/size.php",
			"src/sort.php",
			"src/sum.php",
			"src/take.php",
			"src/takeRight.php",
			"src/tap.php",
			"src/thru.php",
			"src/toArray.php",
			"src/toArrayOrScalar.php",
			"src/union.php",
			"src/values.php",
			"src/where.php",
			"src/without.php"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "^4.7",
		"phpdocumentor/phpdocumentor": "^2.8"
	}
}

{
	"name": "mpetrovich/dash",
	"description": "A functional utility library for PHP",
	"keywords": ["php", "functional programming", "functional", "underscore", "lodash", "underbar", "bottomline", "ramda"],
	"homepage": "https://github.com/mpetrovich/Dash",
	"license": "MIT",
	"authors": [
		{
			"name": "Mike Petrovich",
			"email": "michael.c.petrovich@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.0"
	},
	"autoload": {
		"psr-4": {
			"Dash\\": ["src/", "tests/"]
		},
		"files": [
			"src/Collection/each.php",
			"src/Collection/get.php",
			"src/Collection/map.php",
			"src/Collection/mapValues.php",
			"src/Collection/property.php"
		]
	},
	"require-dev": {
		"phpunit/phpunit": "^4.7"
	}
}
